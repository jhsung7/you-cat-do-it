VITE_API_URL=http://localhost:3001
# Proxy endpoint that calls Gemini server-side (keep the real key in server env GEMINI_API_KEY)
# For Vercel same-origin, you can set this to "/api/gemini"
VITE_GEMINI_PROXY_URL=/api/gemini
# Optional: shared state write token (must match server STATE_WRITE_TOKEN if set)
VITE_STATE_WRITE_TOKEN=
